{
  "name": "top-6",
  "version": "2.6.0",
  "description": "Process which compute top 6.",
  "private": true,
  "engines": {
    "node": "18"
  },
  "main": "build/src/functions.js",
  "scripts": {
    "postinstall": "ts-patch install -s",
    "start": "node build/src/main.js",
    "start:dev": "ts-node-dev --respawn --transpile-only src/main.ts",
    "clean": "rimraf coverage build tmp",
    "prebuild": "npm run lint",
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -w -p tsconfig.json",
    "build:release": "npm run clean && tsc -p tsconfig.release.json",
    "lint": "eslint . --ext .ts",
    "test": "jest --coverage --runInBand",
    "test:watch": "jest --watch",
    "generate:openapi": "openapi-generator-cli generate -i http://localhost:3004/doc-json -g typescript-axios -o src/common/tabt-client --additional-properties enumNameSuffix=tabt --additional-properties useSingleRequestParameter=true",
    "release": "release-it"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "2.7.0",
    "@types/jest": "29.5.14",
    "@types/lodash": "4.14.198",
    "@types/node": "20.6.0",
    "@typescript-eslint/eslint-plugin": "6.7.0",
    "@typescript-eslint/parser": "6.7.0",
    "bun-types": "^1.0.1",
    "eslint": "8.49.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-jest": "27.2.3",
    "firebase-functions": "^4.4.1",
    "jest": "29.7.0",
    "jest-ts-auto-mock": "2.1.0",
    "prettier": "3.0.3",
    "release-it": "16.1.5",
    "rimraf": "5.0.1",
    "ts-auto-mock": "3.7.1",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "2.0.0",
    "tsutils": "3.21.0",
    "ts-patch": "3.0.2",
    "typescript": "5.2.2"
  },
  "author": "Florent Cardoen <Fllorent0D@users.noreply.github.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "@release-it/conventional-changelog": "7.0.1",
    "axios": "1.5.0",
    "axios-retry": "3.7.0",
    "chalk": "4.1.2",
    "date-fns": "2.30.0",
    "dotenv": "16.3.1",
    "exceljs": "4.3.0",
    "fb": "2.0.0",
    "firebase-admin": "11.10.1",
    "lodash": "4.17.21",
    "node-mailjet": "6.0.4",
    "pino": "8.15.1",
    "pino-pretty": "10.2.0",
    "pino-tee": "0.3.0",
    "random-ipv4": "1.0.2",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1",
    "tslib": "2.6.2",
    "type-flag": "3.0.0",
    "typedi": "0.10.0"
  },
  "volta": {
    "node": "16.13.0"
  }
}
